<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns:fx="https://github.com/henninltn"
            fx:controller="com.github.henninltn.sheet.Controller"
            id="BorderPane" prefHeight="400" prefWidth="600">
